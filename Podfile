platform :ios, '9.0'
use_frameworks!

def install_pods
  pod 'Realm', '2.5.0'
  pod 'FMDB', '2.6.2'
  pod 'AlamofireSwiftyJSON', '0.2.0'
  pod 'SwiftyJSON', '3.1.4'
  pod 'Alamofire', '4.4.0'
end

target 'ios-infrastructures-demo' do
  install_pods
  # Pods for ios-infrastructures-demo

  target 'ios-infrastructures-demoTests' do
    install_pods
    # Pods for testing
  end

end
